    "discord.js": "^14.17.3",
    "discordx": "^11.12.2",
    "dotenv": "^17.0.0",
    "express": "^5.1.0",
    "ms": "^2.1.3",
    "node-fetch": "^3.3.2",
    "node-llama-cpp": "^3.5.0",
    "ollama": "^0.6.0",
    "pg": "^8.13.2",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/ms": "^2.1.0",
    "@types/node": "^24.0.2",
    "@types/pg": "^8.11.11",
    "nodemon": "^3.1.9",
    "prettier": "^3.4.2",
    "ts-node": "^10.9.2",
    "typescript": "5.9.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  }
