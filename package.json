{
  "name": "omega-bot-typescript",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "main": "build/main.js",
  "scripts": {
    "build": "tsc",
    "dev": "node --loader ts-node/esm/transpile-only src/main.ts",
    "start": "node build/main.js",
    "watch": "nodemon --exec npm run dev --watch src --ext ts",
    "startDatabase": "docker-compose up -d && start"
